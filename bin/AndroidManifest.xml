<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wxxiaomi.teachingoffice2"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="16" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/Theme.Sherlock.Light" >
        <activity
           
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.wxxiaomi.teachingoffice2.view.activity.OfficeActivity"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.wxxiaomi.teachingoffice2.view.activity.OfficeUserInfoEditActivity"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.wxxiaomi.teachingoffice2.view.activity.OfficeLoginActivity"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.wxxiaomi.teachingoffice2.view.activity.LibraryLoginActivity"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.wxxiaomi.teachingoffice2.view.activity.LibSearchActivity"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.wxxiaomi.teachingoffice2.view.activity.LibBookInfoActivity"
            android:screenOrientation="portrait" >
        </activity>
    </application>

</manifest>